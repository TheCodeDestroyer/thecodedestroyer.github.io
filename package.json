{
  "name": "thecodedestroyer.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "base:prettier": "prettier './**/*.{ts,tsx,html,json,md,css}' --config .prettierrc --cache",
    "base:eslint": "eslint ./ --ext .js,.ts,.tsx --cache",
    "lint:prettier": "pnpm base:prettier --check",
    "lint:eslint": "pnpm base:eslint",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "format:prettier": "pnpm base:prettier --write",
    "format:eslint": "pnpm base:eslint --fix",
    "format": "pnpm format:prettier && pnpm format:eslint",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "1.7.18",
    "@heroicons/react": "2.1.3",
    "class-variance-authority": "0.7.0",
    "framer-motion": "11.0.25",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18",
    "react-intersection-observer": "9.8.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.0.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "autoprefixer": "^10.0.1",
    "clsx": "2.1.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-tcd": "1.3.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss": "^8",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
